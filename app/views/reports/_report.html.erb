<div id="<%= dom_id report %>">
  <p>
    <strong>Title:</strong>
    <%= report.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= report.content %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= l report.created_at, format: :long %>
  </p>

  <p>
    <strong>Contributor:</strong>
    <%= report.user.name %>
  </p>

</div>
